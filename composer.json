{
    "name": "fesor/di_magic",
    "description": "Adds some nasty magic for symfony dependency injection",
    "type": "library",
    "require": {
        "symfony/dependency-injection": "^3.1",
        "symfony/config": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Fesor\\DIMagic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fesor\\DIMagic\\Tests\\Fixtures\\": "tests/fixtures",
            "Fesor\\DIMagic\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Protko",
            "email": "fesors@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
